{
  "include": [],
  "compilerOptions": {
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "~*": ["./src/*"]
    },
    "types": ["vue", "ant-design-vue"]
  }
}
